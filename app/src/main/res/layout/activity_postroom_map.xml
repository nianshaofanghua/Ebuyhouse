<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

   <FrameLayout
       android:id="@+id/frame"
       android:layout_width="match_parent"
       android:layout_height="match_parent">
       <fragment
           android:id="@+id/postroom_map"
           android:layout_width="match_parent"
           android:layout_height="match_parent"
           android:name="com.google.android.gms.maps.MapFragment" />
   </FrameLayout>

    <FrameLayout
        android:id="@+id/postroom_map_yincang"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#00000000"
        android:visibility="gone"/>

    <ImageView
        android:id="@+id/postroom_map_back"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:src="@mipmap/jiantou_left"
        android:scaleType="center"
        android:background="@color/text_red"
        android:layout_margin="5dp"
        android:layout_below="@+id/postroom_map_yincang"/>
<ImageView
    android:layout_centerInParent="true"
    android:src="@mipmap/address_tagert"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"/>
    <TextView
        android:id="@+id/postroom_map_submit"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:text="Submit"
        android:textColor="@color/baise"
        android:background="@color/text_red"
        android:gravity="center"
        android:layout_alignParentBottom="true"
        android:layout_margin="30dp"/>

</RelativeLayout>
