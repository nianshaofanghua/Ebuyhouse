<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.yidankeji.cheng.ebuyhouse.myinfomodule.activity.FundCertificateActivity">
    <FrameLayout
        android:id="@+id/myzijindata_yincang"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/baise"
        android:visibility="gone"/>
    <include layout="@layout/action_bar_title02"/>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:background="@color/baise"
        android:orientation="vertical">

        <TextView
            android:id="@+id/myzijindata_money"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:gravity="center_vertical"
            android:paddingLeft="10dp"
            android:text="Fund certificate"
            android:textColor="@color/text_heise"/>
<View
    android:background="@color/text_hui"
    android:layout_width="match_parent"
    android:layout_height="1dp"/>
        <android.support.v7.widget.RecyclerView
            android:id="@+id/item_submitroom_xiangce"
            android:layout_width="match_parent"
            android:layout_height="100dp"/>





    </LinearLayout>


    <TextView
        android:id="@+id/submit"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:text="Submit"
        android:textSize="20dp"
        android:textColor="@color/baise"
        android:gravity="center"
        android:background="@drawable/shape_text_bg_zhutise"
        android:layout_marginRight="30dp"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="60dp"/>
</LinearLayout>
