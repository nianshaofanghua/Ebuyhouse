<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.yidankeji.cheng.ebuyhouse.myinfomodule.activity.NewMyRenttalRomActivity">

    <FrameLayout
        android:id="@+id/myrentalroom_yincang"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/baise"
        android:visibility="gone"/>
    <include layout="@layout/action_bar_title02"/>

    <android.support.design.widget.TabLayout
        android:id="@+id/savelist_tablayout"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        app:tabMode="scrollable"
        android:paddingLeft="-10dp"
        android:paddingRight="-10dp"
        app:tabPadding="-10dp"
        app:tabTextAppearance="@android:style/TextAppearance.Widget.TabWidget"
        app:tabSelectedTextColor="@color/zhutise"
        app:tabTextColor="@color/text_hei"
        app:tabIndicatorColor="@color/zhutise"/>

    <android.support.v4.view.ViewPager
        android:id="@+id/savelist_viewpager"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
</LinearLayout>
